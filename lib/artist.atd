type external_urls <ocaml from="External_urls"> = abstract

type artist = {
  external_urls: external_urls;
  href: string;
  id: string;
  name: string;
  uri: string;
}

type search = {
  href: string;
  items: artist list;
  limit: int;
  offset: int;
  total: int;
}

type search_wrapper = {
  artists: search;
}
