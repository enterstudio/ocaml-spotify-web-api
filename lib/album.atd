type album_simplified = {
  album_type: string;
  available_markets: string list;
  href: string;
  id: string;
  name: string;
  uri: string;
}

type search = {
  href: string;
  items: album_simplified list;
  limit: int;
  offset: int;
  total: int;
}

type search_wrapper = {
  albums: search;
}
